{
    "name": "godoremi",
    "version": "0.0.1",
    "description": "Golang daemon for docker remote API",
    "http": {
        "listen": "localhost",
        "port": "8080"
    },
    "upstream": {
        "unixSocket":{
            "docker": {
                "fileDescriptor": "/var/run/docker.sock"
            }
        }
    }
}